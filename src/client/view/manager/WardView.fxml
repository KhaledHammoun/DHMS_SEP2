<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.manager.WardViewController">
    <children>
        <Button layoutX="32.0" layoutY="200.0" mnemonicParsing="false" onAction="#onAddWardButton" prefHeight="25.0" prefWidth="59.0" text="Add" />
        <Button layoutX="32.0" layoutY="245.0" mnemonicParsing="false" onAction="#onEditWardButton" prefHeight="25.0" prefWidth="59.0" text="Edit" />
        <Button layoutX="32.0" layoutY="289.0" mnemonicParsing="false" onAction="#onRemoveWardButton" text="Remove" />
        <TableView fx:id="wardTable" layoutX="200.0" layoutY="24.0" prefHeight="446.0" prefWidth="487.0">
            <columns>
                <TableColumn fx:id="wardNameColumn" prefWidth="338.0" text="Ward name" />
                <TableColumn fx:id="roomNumberColumn" prefWidth="148.0" text="Room number" />
            </columns>
        </TableView>
      <Button layoutX="24.0" layoutY="445.0" mnemonicParsing="false" onAction="#onBackButton" text="Back" />
    </children>
</AnchorPane>
