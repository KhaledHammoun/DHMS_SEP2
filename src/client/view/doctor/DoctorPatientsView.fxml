<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.doctor.DoctorPatientsViewController">
   <children>
      <TableView fx:id="doctorViewPatientTable" layoutX="17.0" layoutY="42.0" prefHeight="306.0" prefWidth="544.0">
        <columns>
          <TableColumn fx:id="doctorViewPatientSSN" prefWidth="170.0" text="SSN" />
          <TableColumn fx:id="doctorViewPatientFirstName" prefWidth="182.0" text="First Name" />
            <TableColumn fx:id="doctorViewPatientLastName" prefWidth="190.0" text="Last Name" />
        </columns>
      </TableView>
      <Label fx:id="doctorViewPatientLabel" layoutX="17.0" layoutY="8.0" text="Patient">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="doctorViewTreatButton" layoutX="246.0" layoutY="361.0" mnemonicParsing="false" onAction="#onTreatButton" text="Treat" />
      <Button fx:id="doctorViewAddSampleButton" layoutX="335.0" layoutY="361.0" mnemonicParsing="false" onAction="#onAddSample" text="Add/Edit sample" />
      <Button fx:id="doctorViewDiagnoseButton" layoutX="467.0" layoutY="361.0" mnemonicParsing="false" onAction="#onDiagnoseButton" text="Diagnose" />
      <Button fx:id="doctorViewEditMedicalDescription" layoutX="45.0" layoutY="361.0" mnemonicParsing="false" onAction="#onEditMedicalDescription" text="Edit medical description" />
   </children>
</AnchorPane>
