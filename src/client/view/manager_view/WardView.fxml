<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.manager_view.WardViewController">
    <children>
        <Button layoutX="23.0" layoutY="99.0" mnemonicParsing="false" onAction="#onAddWardButton" prefHeight="25.0" prefWidth="59.0" text="Add" />
        <Button layoutX="23.0" layoutY="144.0" mnemonicParsing="false" onAction="#onEditWardButton" prefHeight="25.0" prefWidth="59.0" text="Edit" />
        <Button layoutX="23.0" layoutY="188.0" mnemonicParsing="false" onAction="#onRemoveWardButton" text="Remove" />
        <TableView fx:id="wardTable" layoutX="200.0" layoutY="24.0" prefHeight="357.0" prefWidth="378.0">
            <columns>
                <TableColumn fx:id="wardNameColumn" prefWidth="193.0" text="Ward name" />
                <TableColumn fx:id="roomNumberColumn" prefWidth="184.0" text="Room number" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
