<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.manager_view.EmployeeViewController">
   <children>
      <ComboBox fx:id="employeeTypeComboBox" layoutX="23.0" layoutY="24.0" prefHeight="25.0" prefWidth="105.0" promptText="Select type" />
      <Button layoutX="23.0" layoutY="99.0" mnemonicParsing="false" onAction="#onAddEmployeeButton" prefHeight="25.0" prefWidth="59.0" text="Add" />
      <Button layoutX="23.0" layoutY="144.0" mnemonicParsing="false" onAction="#onEditEmployeeButton" prefHeight="25.0" prefWidth="59.0" text="Edit" />
      <Button layoutX="23.0" layoutY="188.0" mnemonicParsing="false" onAction="#onRemoveEmployeeButton" text="Remove" />
      <TableView fx:id="employeeTable" layoutX="200.0" layoutY="24.0" prefHeight="357.0" prefWidth="378.0">
        <columns>
          <TableColumn fx:id="ssnColumn" prefWidth="101.0" text="SSN" />
          <TableColumn fx:id="fNameColumn" prefWidth="129.0" text="First name" />
            <TableColumn fx:id="lNameColumn" prefWidth="147.0" text="Last name" />
        </columns>
      </TableView>
      <Button layoutX="136.0" layoutY="24.0" mnemonicParsing="false" onAction="#onGetButton" text="Get" />
   </children>
</AnchorPane>
